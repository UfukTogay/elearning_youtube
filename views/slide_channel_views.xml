<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="slide_channel_view_form_inherit_youtube_import" model="ir.ui.view">
            <field name="name">slide.channel.view.form.inherit.youtube.import</field>
            <field name="model">slide.channel</field>
            <field name="inherit_id" ref="website_slides.view_slide_channel_form"/>
            <field name="arch" type="xml">
                <xpath expr="//header" position="inside">
                    <button name="action_open_import_youtube_wizard"
                            string="Import from YouTube"
                            type="object"
                            class="btn-primary"/>
                </xpath>
            </field>
        </record>
    </data> 
</odoo>
