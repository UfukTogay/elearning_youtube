<odoo>
    <record id="youtube_playlist_import_wizard_view_form" model="ir.ui.view">
        <field name="name">youtube.playlist.import.wizard.view.form</field>
        <field name="model">youtube.playlist.import.wizard</field>
        <field name="arch" type="xml">
            <form string="Import YouTube Playlist">
                <group>
                    <field name="playlist_url"/>
                </group>
                <footer>
                    <button name="action_import" string="Fetch and Import" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>